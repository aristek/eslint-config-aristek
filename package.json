{
  "name": "eslint-config-aristek",
  "version": "5.0.0",
  "description": "ESLint config based on team guideline and experience.",
  "repository": "https://github.com/aristek/eslint-config-aristek",
  "license": "MIT",
  "main": "index.js",
  "files": [
    "base.js",
    "index.js"
  ],
  "peerDependencies": {
    "@babel/eslint-parser": "^7.12.1",
    "eslint": "^7.12.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-only-warn": "^1.0.2",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0"
  },
  "optionalDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0"
  },
  "devDependencies": {
    "@commitlint/cli": "12.0.0",
    "@commitlint/config-conventional": "12.0.0",
    "husky": "4.2.1",
    "lint-staged": "10.0.4",
    "prettier": "1.19.1",
    "semantic-release": "17.2.3",
    "semantic-release-config-aristek": "1.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": "prettier --write"
  },
  "release": {
    "extends": "semantic-release-config-aristek"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
