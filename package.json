{
  "name": "@aristek/eslint-config",
  "version": "4.7.0",
  "description": "ESLint config based on team guideline and experience.",
  "repository": "https://git.aristeksystems.com/js/configs/eslint.git",
  "license": "UNLICENSED",
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "peerDependencies": {
    "eslint": "~6.6.0",
    "eslint-config-airbnb": "~18.0.1",
    "eslint-config-prettier": "~6.6.0",
    "eslint-plugin-import": "~2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-only-warn": "~1.0.2",
    "eslint-plugin-react": "~7.18.3",
    "eslint-plugin-react-hooks": "~1.7.0",
    "semantic-release-config-aristek": "file:.yalc/semantic-release-config-aristek"
  },
  "dependencies": {
    "babel-eslint": "10.0.3"
  },
  "optionalDependencies": {
    "@typescript-eslint/eslint-plugin": "2.10.0",
    "@typescript-eslint/parser": "2.10.0"
  },
  "devDependencies": {
    "@commitlint/cli": "8.3.4",
    "@commitlint/config-conventional": "8.3.4",
    "husky": "4.0.9",
    "lint-staged": "9.5.0",
    "prettier": "1.19.1",
    "semantic-release": "17.0.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "release": {
    "extends": "semantic-release-config-aristek"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
